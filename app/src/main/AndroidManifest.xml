<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.demoexam">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_logo"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/icon_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.DemoExam"
        android:usesCleartextTraffic="true">
        <activity android:name=".MainScreen"></activity>
        <activity android:name=".screen.LaunchScreen.SignUpScreen.SignUp" />
        <activity android:name=".screen.LaunchScreen.SignInScreen.SignIn" />
        <activity android:name=".screen.LaunchScreen.LaunchScreen" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>